{
  "name": "akisakye-matomo",
  "version": "0.1.9",
  "author": "Alex Kisakye",
  "summary": "Puppet Matomo Web Analytics platform installation module",
  "license": "GPL-3.0-only",
  "source": "https://github.com/akisakye/matomo.git",
  "project_page": "https://github.com/akisakye/matomo",
  "issues_url": "https://github.com/akisakye/matomo/issues",
  "dependencies": [
    {
      "name": "puppet/archive",
      "version_requirement": ">= 7.1.0 < 7.1.1"
    },
    {
      "name": "puppet/nginx",
      "version_requirement": ">= 6.0.0 < 6.0.1"
    },
    {
      "name": "puppet/php",
      "version_requirement": ">= 10.2.0 < 10.2.1"
    },
    {
      "name": "puppetlabs/mysql",
      "version_requirement": ">= 16.0.0 < 16.0.1"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "20.10",
        "21.04",
        "21.10",
        "22.04",
        "22.10",
        "23.04",
        "23.10"
      ]
    },
    {
      "operatingsystem": "AlmaLinux",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.10.0 < 8.0.0"
    }
  ]
}
